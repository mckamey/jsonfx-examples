<?xml version="1.0"?>
<configuration>
	<connectionStrings/>
	<system.web>
		<compilation debug="false">
			<buildProviders>
				<add extension=".jsonfx" type="JsonFx.Compilation.JsonFxScriptBuildProvider, JsonFx" />
				<add extension=".json" type="JsonFx.Compilation.JsonServiceBuildProvider, JsonFx"/>
				<remove extension=".js" />
				<add extension=".js" type="JsonFx.Compilation.ScriptCompactorBuildProvider, JsonFx"/>
			</buildProviders>
			<assemblies>
				<add assembly="System.Web.RegularExpressions, Version=2.0.0.0, Culture=neutral, PublicKeyToken=B03F5F7F11D50A3A"/>
			</assemblies>
		</compilation>
		<httpHandlers>
			<add path="*.jsonfx" verb="*" type="JsonFx.Handlers.ClientScriptHandler, JsonFx" validate="true"/>
			<add path="*.js" verb="*" type="JsonFx.Handlers.ClientScriptHandler, JsonFx" validate="true"/>
			<add path="*.json" verb="*" type="JsonFx.Handlers.JsonServiceHandlerFactory, JsonFx" validate="true"/>
		</httpHandlers>
		<authentication mode="Forms"/>
		<customErrors mode="Off"/>
	</system.web>
</configuration>
