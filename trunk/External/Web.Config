<?xml version="1.0"?>
<configuration>
	<appSettings>
		<add key="GoogleAnalyticsID" value=""/>
	</appSettings>
	<system.web>
		<pages>
			<controls>
				<add tagPrefix="JsonFx" namespace="JsonFx.History" assembly="JsonFx.History"/>
			</controls>
		</pages>
		<!-- set debug="false" to get script compaction -->
		<customErrors mode="RemoteOnly"/>
		<compilation debug="false">
			<buildProviders>
				<remove extension=".css"/>
				<add extension=".css" type="JsonFx.Compilation.ResourceBuildProvider, JsonFx.Client"/>
				<remove extension=".js"/>
				<add extension=".js" type="JsonFx.Compilation.ResourceBuildProvider, JsonFx.Client"/>
				<add extension=".merge" type="JsonFx.Compilation.ResourceBuildProvider, JsonFx.Client"/>
				<add extension=".jbst" type="JsonFx.Compilation.ResourceBuildProvider, JsonFx.Client"/>
				<add extension=".jsonrpc" type="JsonFx.Compilation.JsonServiceBuildProvider, JsonFx.IO"/>
			</buildProviders>
		</compilation>
		<httpHandlers>
			<add path="*.css" verb="*" type="JsonFx.Handlers.ResourceHandler, JsonFx.Client" validate="true"/>
			<add path="*.js" verb="*" type="JsonFx.Handlers.ResourceHandler, JsonFx.Client" validate="true"/>
			<add path="*.merge" verb="*" type="JsonFx.Handlers.ResourceHandler, JsonFx.Client" validate="true"/>
			<add path="*.jbst" verb="*" type="JsonFx.Handlers.ResourceHandler, JsonFx.Client" validate="true"/>
			<add path="*.jsonrpc" verb="*" type="JsonFx.Handlers.JsonServiceHandlerFactory, JsonFx.IO" validate="true"/>
		</httpHandlers>
		<httpModules>
			<add name="JsonMLBrowserModule" type="JsonFx.JsonML.JsonMLBrowserModule, JsonFx.UI"/>
		</httpModules>
		<xhtmlConformance mode="Strict"/>
	</system.web>
	<system.codedom>
		<compilers>
			<compiler language="css;stylesheet" extension=".css" type="JsonFx.Compilation.CssResourceCodeProvider, JsonFx.Client"/>
			<compiler language="js;javascript" extension=".js" type="JsonFx.Compilation.ScriptResourceCodeProvider, JsonFx.Client"/>
			<compiler language="jbst;template" extension=".jbst" type="JsonFx.Compilation.JbstCodeProvider, JsonFx.UI"/>
			<compiler language="merge;resources"  extension=".merge" type="JsonFx.Compilation.MergeResourceCodeProvider, JsonFx.Client"/>
		</compilers>
	</system.codedom>
</configuration>
